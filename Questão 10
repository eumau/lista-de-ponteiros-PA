#include <stdio.h>
int main(){
    printf("INT\n");
    int x[4];
    printf("%d\n", x+1);
    printf("%d\n", x+2);
    printf("%d\n", x+3);
    printf("CHARn");
    char y[4];
    printf("%d\n", y+1);
    printf("%d\n", y+2);
    printf("%d\n", y+3);
    printf("FLOAT\n");
    float a[4];
    printf("%d\n", a+1);
    printf("%d\n", a+2);
    printf("%d\n", a+3);
    printf("CHAR\n");
    double b[4];
    printf("%d\n", b+1);
    printf("%d\n", b+2);
    printf("%d\n", b+3);
    return 0;
}
